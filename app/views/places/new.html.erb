<div class="booyah-box col-10 offset-1">
  <h1>Enter the details of your favorite restaurant!</h1>
  <%= simple_form_for @place do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <br/>
    <%= f.submit 'Submit your suggestion!', class: 'btn btn-primary btn-lg' %>
  <% end %>
</div>